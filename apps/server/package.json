{
  "name": "@kingshare/server",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "build": "npm run clean && tsc && tsc-alias",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate deploy",
    "prisma:seed": "npx prisma db seed",
    "seed:prod": "node scripts/seed-production.js",
    "setup:prod": "npm run prisma:generate && npm run prisma:migrate && npm run seed:prod",
    "deploy:heroku": "chmod +x scripts/deploy-heroku.sh && scripts/deploy-heroku.sh",
    "deploy:quick": "chmod +x scripts/quick-deploy.sh && scripts/quick-deploy.sh",
    "deploy:staging": "scripts/deploy-heroku.sh --app qub-drive-staging",
    "deploy:prod": "scripts/deploy-heroku.sh --app qub-drive-production --no-skip-tests",
    "prestart": "npm run build",
    "docker:build": "docker build -t qub-drive .",
    "docker:run": "docker run -p 3000:3000 qub-drive"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/busboy": "^1.5.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.5.2",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/pdf-parse": "^1.1.5",
    "@types/pg": "^8.15.4",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.16.2",
    "rimraf": "^6.0.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@kingshare/shared-types": "*",
    "@kingshare/utils": "*",
    "@prisma/client": "^6.12.0",
    "@types/bcrypt": "^6.0.0",
    "@types/multer": "^2.0.0",
    "@types/socket.io": "^3.0.1",
    "archiver": "^7.0.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "busboy": "^1.6.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.5",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "prisma": "^6.12.0",
    "qrcode": "^1.5.4",
    "rate-limiter-flexible": "^7.1.1",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts"
  }
}
